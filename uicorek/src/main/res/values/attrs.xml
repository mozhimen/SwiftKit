<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--btnk-->
    <declare-styleable name="BtnKIconFont">
        <attr name="btnKIconFont_fontPath" format="string" />
    </declare-styleable>

    <declare-styleable name="BtnKPwdEye">
