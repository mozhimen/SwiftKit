        <attr name="imageKSquare_measureMode" format="enum">
            <enum name="measureMin" value="0" />
            <enum name="measureMax" value="1" />
            <enum name="measureWidth" value="2" />
            <enum name="measureHeight" value="3" />
        </attr>
    </declare-styleable>

    <!--layoutk_search-->
    <attr name="LayoutKSearch_LayoutKSearch_Style" format="reference" />
    <declare-styleable name="LayoutKSearch">
        <attr name="layoutKSearch_search_icon" format="string|reference" />
        <attr name="layoutKSearch_search_iconSize" format="dimension|reference" />
        <attr name="layoutKSearch_search_iconPadding" format="dimension|reference" />
        <attr name="layoutKSearch_search_background" format="reference" />
        <attr name="layoutKSearch_search_textSize" format="dimension|reference" />
        <attr name="layoutKSearch_search_textColor" format="color|reference" />

        <attr name="layoutKSearch_clear_icon" format="string|reference" />
        <attr name="layoutKSearch_clear_iconSize" format="dimension|reference" />

        <attr name="layoutKSearch_hint_text" format="string|reference" />
        <attr name="layoutKSearch_hint_textSize" format="dimension|reference" />
        <attr name="layoutKSearch_hint_textColor" format="color|reference" />
        <attr name="layoutKSearch_hint_gravity" format="enum">
            <enum name="center" value="0" />
            <enum name="left" value="1" />
        </attr>

        <attr name="layoutKSearch_keyword_icon" format="string|reference" />
        <attr name="layoutKSearch_keyword_iconSize" format="dimension|reference" />
        <attr name="layoutKSearch_keyword_iconColor" format="color|reference" />
        <attr name="layoutKSearch_keyword_background" format="reference" />
        <attr name="layoutKSearch_keyword_maxLength" format="integer|reference" />
        <attr name="layoutKSearch_keyword_padding" format="dimension|reference" />
    </declare-styleable>

    <!--layoutk_navbar-->
    <attr name="LayoutKNavBar_LayoutKNavBar_Style" format="reference" />
    <declare-styleable name="LayoutKNavBar">
        <attr name="layoutKNavBar_title" format="string|reference" />
        <attr name="layoutKNavBar_title_textColor" format="color|reference" />
        <attr name="layoutKNavBar_title_textSize" format="dimension|reference" />

        <attr name="layoutKNavBar_subTitle" format="string|reference" />
        <attr name="layoutKNavBar_subTitle_textSize" format="dimension|reference" />
        <attr name="layoutKNavBar_subTitle_textColor" format="color|reference" />
        <attr name="layoutKNavBar_subTitle_marginTop" format="dimension|reference" />

        <attr name="layoutKNavBar_lineColor" format="color|reference" />
        <attr name="layoutKNavBar_lineWidth" format="dimension|reference" />
    </declare-styleable>

    <!--layoutk_banner-->
    <declare-styleable name="LayoutKBanner">
        <attr name="layoutKBanner_autoPlay" format="boolean" />
        <attr name="layoutKBanner_loop" format="boolean" />
        <attr name="layoutKBanner_enableScroll" format="boolean" />
        <attr name="layoutKBanner_scrollDuration" format="integer" />
        <attr name="layoutKBanner_intervalTime" format="integer" />
    </declare-styleable>

    <!--layoutk_slider-->
    <declare-styleable name="LayoutKSlider">
        <attr name="layoutKSlider_sliderHeight" format="reference|dimension" />
        <attr name="layoutKSlider_sliderRodLeftColor" format="color|reference" />
        <attr name="layoutKSlider_sliderRodLeftGradientColor" format="color|reference" />
        <attr name="layoutKSlider_sliderRodRightColor" format="color|reference" />
        <attr name="layoutKSlider_rodScrollEnable" format="boolean" />
        <attr name="layoutKSlider_rodColor" format="color|reference" />
        <attr name="layoutKSlider_rodColorInside" format="reference|color" />
        <attr name="layoutKSlider_rodIsInside" format="boolean" />
        <attr name="layoutKSlider_rodRadius" format="dimension|reference" />
        <attr name="layoutKSlider_rodRadiusInside" format="dimension|reference" />
        <attr name="layoutKSlider_rodMinVal" format="float" />
        <attr name="layoutKSlider_rodMaxVal" format="float" />
        <attr name="layoutKSlider_rodDefaultPercent" format="float" />
    </declare-styleable>

    <declare-styleable name="LayoutKEmpty">
        <attr name="layoutKEmpty_iconFont" format="string|reference" />
        <attr name="layoutKEmpty_image" format="reference" />
        <attr name="layoutKEmpty_title" format="string|reference" />
        <attr name="layoutKEmpty_content" format="string|reference" />
        <attr name="layoutKEmpty_helpIconFont" format="string|reference" />
        <attr name="layoutKEmpty_buttonTitle" format="string|reference" />
    </declare-styleable>

    <declare-styleable name="LayoutKAmount">
        <attr name="layoutKAmount_minVal" format="integer" />
        <attr name="layoutKAmount_maxVal" format="integer" />
        <attr name="layoutKAmount_defaultAmount" format="integer" />

        <attr name="layoutKAmount_amountTextColor" format="color|reference" />
        <attr name="layoutKAmount_amountTextSize" format="dimension|reference" />
        <attr name="layoutKAmount_amountBackgroundColor" format="color|reference" />
        <attr name="layoutKAmount_amountMarginHorizontal" format="dimension|reference" />
        <attr name="layoutKAmount_amountMinWidth" format="dimension|reference" />

        <attr name="layoutKAmount_btnTextColor" format="color|reference" />
        <attr name="layoutKAmount_btnTextSize" format="dimension|reference" />
        <attr name="layoutKAmount_btnBackgroundColor" format="color|reference" />
        <attr name="layoutKAmount_btnSize" format="dimension|reference" />
    </declare-styleable>

    <!--视频布局-->
    <declare-styleable name="LayoutKVideo">
        <!--视频地址-->
        <attr name="layoutKVideo_pathOrUrl" format="string|reference" />
        <!--视频是否循环播放-->
        <attr name="layoutKVideo_isLoop" format="boolean" />
        <!--视频位置权重-->
        <attr name="layoutKVideo_videoGravity" format="enum">
            <enum name="start" value="0" />
            <enum name="end" value="1" />
            <enum name="centerCrop" value="2" />
            <enum name="none" value="3" />
        </attr>
        <!--音量0-10-->
        <attr name="layoutKVideo_volume" format="integer" />
    </declare-styleable>

    <!--毛玻璃布局-->
    <declare-styleable name="LayoutKBlur">
        <!--图片src-->
        <attr name="layoutKBlur_imageSrc" format="reference" />
        <!--是否图片移动-->
        <attr name="layoutKBlur_isMoveEnable" format="boolean" />
        <!--是否禁用模糊-->
        <attr name="layoutKBlur_isBlurEnable" format="boolean" />
    </declare-styleable>

    <declare-styleable name="LayoutKEditItem">
        <attr name="layoutKEditItem_hint" format="string|reference" />
        <attr name="layoutKEditItem_title" format="string|reference" />
        <attr name="layoutKEditItem_inputType" format="enum">
            <enum name="text" value="0" />
            <enum name="password" value="1" />
            <enum name="number" value="2" />
        </attr>
        <attr name="layoutKEditItem_titleAppearance" format="reference" />
        <attr name="layoutKEditItem_inputAppearance" format="reference" />
        <attr name="layoutKEditItem_topLineAppearance" format="reference" />
        <attr name="layoutKEditItem_bottomLineAppearance" format="reference" />
    </declare-styleable>

    <declare-styleable name="LayoutKEditItem_TitleAppearance">
        <attr name="titleAppearance_titleColor" format="reference|color" />
        <attr name="titleAppearance_titleSize" format="dimension|reference" />
        <attr name="titleAppearance_leftPadding" format="dimension|reference" />
        <attr name="titleAppearance_minWidth" format="dimension" />
    </declare-styleable>

    <declare-styleable name="LayoutKEditItem_EditAppearance">
        <attr name="inputAppearance_hintColor" format="reference|color" />
        <attr name="inputAppearance_inputColor" format="color|reference" />
        <attr name="inputAppearance_textSize" format="dimension|reference" />
        <attr name="inputAppearance_maxEditLength" format="integer" />
    </declare-styleable>

    <declare-styleable name="LayoutKEditItem_LineAppearance">
        <attr name="lineAppearance_color" format="reference|color" />
        <attr name="lineAppearance_height" format="dimension|reference" />
        <attr name="lineAppearance_leftMargin" format="dimension|reference" />
        <attr name="lineAppearance_rightMargin" format="dimension|reference" />
    </declare-styleable>

    <!--layoutk_side-->
    <declare-styleable name="LayoutKSideList">
        <attr name="layoutKSideList_menuWidth" format="dimension|reference" />
        <attr name="layoutKSideList_menuHeight" format="dimension|reference" />
        <attr name="layoutKSideList_menuItemTextSize" format="dimension|reference" />
        <attr name="layoutKSideList_menuItemTextSizeSelect" format="dimension|reference" />
        <attr name="layoutKSideList_menuItemTextColor" format="reference|color" />
        <attr name="layoutKSideList_menuItemTextColorSelect" format="reference|color" />
        <attr name="layoutKSideList_menuItemBackgroundColor" format="reference|color" />
        <attr name="layoutKSideList_menuItemBackgroundColorSelect" format="reference|color" />
        <attr name="layoutKSideList_menuItemIndicator" format="reference" />
        <attr name="layoutKSideList_subTextSize" format="dimension|reference" />
        <attr name="layoutKSideList_subTextColor" format="color|reference" />
        <attr name="layoutKSideList_subHeight" format="dimension|reference" />
        <attr name="layoutKSideList_subMarginStart" format="dimension|reference" />
        <attr name="layoutKSideList_contentTextSize" format="dimension|reference" />
        <attr name="layoutKSideList_contentTextColor" format="color|reference" />
        <attr name="layoutKSideList_contentImgRatio" format="enum">
            <enum name="ratio11" value="0" />
            <enum name="ratio34" value="1" />
            <enum name="ratio169" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="LayoutKBtnSwitch">
        <!--默认开关状态-->
        <attr name="layoutKBtnSwitch_defaultStatus" format="boolean" />
        <!--打开时颜色-->
        <attr name="layoutKBtnSwitch_bgColorOn" format="reference|color" />
        <!--关闭时颜色-->
        <attr name="layoutKBtnSwitch_bgColorOff" format="reference|color" />
        <!--中心圆颜色-->
        <attr name="layoutKBtnSwitch_btnColor" format="reference|color" />
        <!--中心圆到外边距的距离-->
        <attr name="layoutKBtnSwitch_btnMargin" format="dimension|reference" />
        <!--外边框厚度-->
        <attr name="layoutKBtnSwitch_borderWidth" format="dimension|reference" />
        <!--外边框颜色-->
        <attr name="layoutKBtnSwitch_borderColor" format="reference|color" />
        <!--变换时间-->
        <attr name="layoutKBtnSwitch_animTime" format="integer" />

    </declare-styleable>

    <declare-styleable name="LayoutKBtnSwitchApple">
        <!--默认开关状态-->
        <attr name="layoutKBtnSwitchApple_defaultStatus" format="boolean" />
        <!--打开时颜色-->
        <attr name="layoutKBtnSwitchApple_bgColorOn" format="reference|color" />
        <!--关闭时颜色-->
        <attr name="layoutKBtnSwitchApple_bgColorOff" format="reference|color" />
        <!--中心圆颜色-->
        <attr name="layoutKBtnSwitchApple_btnColor" format="reference|color" />
        <!--中心圆到外边距的距离-->
        <attr name="layoutKBtnSwitchApple_btnMargin" format="dimension|reference" />
        <!--外边框厚度-->
        <attr name="layoutKBtnSwitchApple_borderWidth" format="dimension|reference" />
        <!--外边框颜色-->
        <attr name="layoutKBtnSwitchApple_borderColor" format="reference|color" />
        <!--变换时间-->
        <attr name="layoutKBtnSwitchApple_animTime" format="integer" />

    </declare-styleable>

    <declare-styleable name="LayoutKBtnIcon">
        <attr name="layoutKBtnIcon_background" format="reference" />
        <attr name="layoutKBtnIcon_iconSrc" format="reference" />
        <attr name="layoutKBtnIcon_iconSize" format="dimension|reference" />
        <attr name="layoutKBtnIcon_iconMarginRight" format="dimension|reference" />
        <attr name="layoutKBtnIcon_text" format="string|reference" />
        <attr name="layoutKBtnIcon_textColor" format="color|reference" />
        <attr name="layoutKBtnIcon_textSize" format="dimension|reference" />
    </declare-styleable>

    <declare-styleable name="LayoutKSquare">
        <attr name="layoutKSquare_measureMode" format="enum">
            <enum name="measureMin" value="0" />
            <enum name="measureMax" value="1" />
            <enum name="measureWidth" value="2" />
            <enum name="measureHeight" value="3" />
        </attr>
    </declare-styleable>

    <!--textk-->
    <declare-styleable name="TextKBubble">
        <!--设置箭头朝向-->
        <attr name="textKBubble_arrowDirection" format="enum">
            <enum name="none" value="0" />
            <enum name="auto" value="1" />
            <enum name="left" value="2" />
            <enum name="up" value="3" />
            <enum name="right" value="4" />
            <enum name="down" value="5" />
        </attr>
        <!--设置箭头三角形厚度-->
        <attr name="textKBubble_arrowHeight" format="dimension|reference" />
        <!--设置箭头三角形底宽-->
        <attr name="textKBubble_arrowWidth" format="dimension|reference" />
        <!--设置箭头在边线上的位置策略-->
        <attr name="textKBubble_arrowPosPolicy" format="enum">
            <!--箭头指向目标View的中心点-->
            <enum name="target_center" value="0" />
            <!--箭头从自己的中心点发出-->
            <enum name="self_center" value="1" />
            <!--结合setArrowPosOffset，箭头从所在轴的头端开始偏移-->
            <enum name="self_begin" value="2" />
            <!--结合setArrowPosOffset，箭头从所在轴的尾端开始偏移-->
            <enum name="self_end" value="3" />
        </attr>
        <!--设置箭头在所在边线上的偏移距离-->
        <attr name="textKBubble_arrowPosOffset" format="dimension|reference" />
        <!--设置箭头指向的View对象Id-->
        <attr name="textKBubble_arrowToViewId" format="reference" />
        <!--设置气泡背景色-->
        <attr name="textKBubble_bgColor" format="color|reference" />
        <!--设置边框线颜色-->
        <attr name="textKBubble_borderColor" format="color|reference" />
        <!--设置边框线宽-->
        <attr name="textKBubble_borderWidth" format="dimension|reference" />
        <!--设置边框于背景之间的间隙宽度-->
        <attr name="textKBubble_gapPadding" format="dimension|reference" />

        <!--设置边角弧度-->
        <attr name="textKBubble_cornerRadius" format="dimension|reference" />
        <!--设置边角弧度左上角-->
        <attr name="textKBubble_cornerTopLeftRadius" format="dimension|reference" />
        <!--设置边角弧度右上角-->
        <attr name="textKBubble_cornerTopRightRadius" format="dimension|reference" />
        <!--设置边角弧度左下角-->
        <attr name="textKBubble_cornerBottomLeftRadius" format="dimension|reference" />
        <!--设置边角弧度右下角-->
        <attr name="textKBubble_cornerBottomRightRadius" format="dimension|reference" />

        <!--设置内边距-->
        <attr name="textKBubble_padding" format="dimension|reference" />
        <!--设置左右内边距-->
        <attr name="textKBubble_paddingHorizontal" format="dimension|reference" />
        <!--设置上下内边距-->
        <attr name="textKBubble_paddingVertical" format="dimension|reference" />
    </declare-styleable>

    <declare-styleable name="TextKClock">
        <attr name="textkClock_timeFormat" format="enum">
            <enum name="yyyyMMddHHmmss" value="0" />
            <enum name="yyyyMMddHHmm" value="1" />
            <enum name="yyyyMMdd" value="2" />
            <enum name="HHmmss" value="3" />
            <enum name="HHmm" value="4" />
            <enum name="mmss" value="5" />
            <enum name="yyyy" value="6" />
            <enum name="MM" value="7" />
            <enum name="dd" value="8" />
            <enum name="HH" value="9" />
            <enum name="mm" value="10" />
            <enum name="ss" value="11" />
        </attr>
    </declare-styleable>

    <declare-styleable name="TextKEditFocus">
        <attr name="textKEditFocus_focusBackground" format="reference" />
        <attr name="textKEditFocus_unFocusBackground" format="reference" />
    </declare-styleable>

    <declare-styleable name="TextKIconFont">
        <attr name="textKIconFont_fontPath" format="string" />
    </declare-styleable>

    <!--下划线控件-->
    <declare-styleable name="TextKUnderLine">
        <!--下划线是否出现-->
        <attr name="textKUnderLine_underline" format="boolean" />
    </declare-styleable>

    <!--viewk-->
    <declare-styleable name="ViewKWheel">
        <attr name="viewKWheel_gravity">
            <enum name="center" value="17" />
            <enum name="left" value="3" />
            <enum name="right" value="5" />
        </attr>
        <attr name="viewKWheel_textSize" format="dimension|reference" />
        <attr name="viewKWheel_textColorOut" format="color|reference" />
        <attr name="viewKWheel_textColorCenter" format="color|reference" />
        <attr name="viewKWheel_dividerColor" format="color|reference" />
        <attr name="viewKWheel_dividerWidth" format="dimension|reference" />
        <attr name="viewKWheel_lineSpacingMultiplier" format="float" />
    </declare-styleable>

    <declare-styleable name="ViewKScanOverlay">
        <attr name="viewKScanOverlay_isShowLabel" format="boolean" />
        <attr name="viewKScanOverlay_labelTextSize" format="dimension|reference" />
        <attr name="viewKScanOverlay_labelTextColor" format="color|reference" />
        <attr name="viewKScanOverlay_labelBackgroundColor" format="color|reference" />
        <attr name="viewKScanOverlay_boxType" format="enum">
            <enum name="rect" value="0" />
            <enum name="circle" value="1" />
        </attr>
        <attr name="viewKScanOverlay_boxLineWidth" format="dimension|reference" />
        <attr name="viewKScanOverlay_boxLineColor" format="color|reference" />
    </declare-styleable>

    <!--二维码扫描控件-->
    <declare-styleable name="ViewKScanQR">
        <!-- 扫描边框线的宽度，默认值为1dp -->
        <attr name="viewKScanQR_borderLineWidth" format="dimension|reference" />
        <!-- 扫描边框的颜色，默认值为@android:color/white -->
        <attr name="viewKScanQR_borderColor" format="color|reference" />
        <!-- 扫描线的图片资源，默认值为null -->
        <attr name="viewKScanQR_lineDrawable" format="reference" />
        <!-- 扫描线的线宽，默认值为1dp -->
        <attr name="viewKScanQR_lineWidth" format="dimension|reference" />
        <!-- 扫描线从顶部移动到底部的动画时间，默认值为1000 -->
        <attr name="viewKScanQR_animTime" format="integer" />
        <!-- 扫描线是否来回移动，默认值为true -->
        <attr name="viewKScanQR_isScanLineReverse" format="boolean" />
    </declare-styleable>
</resources>